#include<stdio.h>
void func(int size,int row,int col,char anw[size][size]){
if(size==3){
        int n=size/3;
    for(int i=row;i<row+3;i++){
        for(int j=col;j<col+3;j++){
                    anw[i][j]='.';
        }
    }
    anw[row+1][col+1]='#';
}
if(size>3){
    int n=size/3;
    for(int i=row;i<row+3*n;i=i+n){
        for(int j=col;j<col+3*n;j=j+n){
                    func(size/3,i,j,anw);
                }
        }
        for(int i=row+n;i<row+2*n;i++){
            for(int j=col+n;i<col+2*n;j++){
                anw[i][j]='#';
            }
        }
    }
}

int main(){
    int n;
    scanf("%d",&n);

    int size=1;
    for(int i=0;i<n;i++){
        size=size*3;
    }

    char anw[size][size];

    func(size,0,0,anw);

    for(int i=0;i<size;i++){
        for(int j=0;j<size;j++){
           printf("%c",anw[i][j]);
        }
            printf("\n");
    }
return 0;
}
